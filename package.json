{
  "name": "next-starter-kit",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "build": "npm run lint:prod && next build",
    "build-static": "npm run lint:prod && next build && next export",
    "dev": "next",
    "export": "rm -rf .next && npm run build && next export",
    "lint": "eslint --config .eslintrc.js ./src --ext .ts,.tsx,.js --fix",
    "lint:prod": "eslint --config .eslintrc.prod.js ./src --ext .ts,.tsx,.js --fix",
    "lint:hook": "eslint --config .eslintrc.prod.js --fix",
    "lint-staged": "lint-staged --config .lintstagedrc.json",
    "vercel": "vercel --prod",
    "prettier": "prettier --config .prettierrc.json --write \"src/**/*.{ts,tsx}\"",
    "prettier:hook": "prettier --config .prettierrc.json --write",
    "start": "next start",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm run test --silent -- --coverage",
    "test:coverage:only": "npx jest \"$npm_config_src\" \"--coverage\" \"--collectCoverageFrom='$npm_config_src/**/*.(ts|tsx)'\"",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@stitches/react": "0.2.3",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@types/jest": "26.0.24",
    "@types/node": "16.4.9",
    "@types/nprogress": "0.2.0",
    "@types/react-transition-group": "4.4.2",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "acd-utils": "8.0.0",
    "await-to-js": "3.0.0",
    "babel": "6.23.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-loader": "4.0.2",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-standard": "4.1.0",
    "husky": "7.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.6",
    "jest-watch-typeahead": "0.6.4",
    "lint-staged": "11.1.1",
    "next": "11.0.1",
    "nprogress": "0.2.0",
    "postcss": "8.3.6",
    "prettier": "2.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-polymorphic-box": "3.0.3",
    "react-transition-group": "4.4.2",
    "ts-jest": "27.0.4",
    "ts-node": "10.1.0",
    "typescript": "4.3.5",
    "vercel": "23.0.1",
    "zustand": "3.5.7"
  },
  "dependencies": {}
}
